        <script><![CDATA[(function() {
  /* populate the 'data' object */
  /* e.g., data.table = $sp.getValue('table'); */
	
data.userInfo=[];
var gr= new GlideRecord('sys_user');
gr.addQuery('sys_id',gs.getUserID());
gr.Query();
while(gr.Next())
{
data.userInfo.push({
Fahrzeughalter:gr.sys_id.toString(),
Nummernschild:gr.Nummernschild.toString()
});
	

}
//gs.addInomMessage("Name of User"+ datauserInfo[0].Nummernschild);
	
})();]]></script>
